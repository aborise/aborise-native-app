{
  "cli": {
    "version": ">= 5.5.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development"
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview"
    },
    "production": {
      "channel": "production",
      "android": {
        "serviceAccountKeyPath": "./aborise-serviceAccount.json"
      },
      "ios": {
        "appleId": "leo@aborise.com"
      }
    },
    "development-simulator": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "channel": "development-simulator"
    },
    "prod-simulator": {
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "channel": "prod-simulator"
    },
    "dev-android": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "channel": "dev-android"
    }
  },
  "submit": {
    "production": {}
  }
}
